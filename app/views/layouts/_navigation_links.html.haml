- if !signed_in?
  = link_to 'Sign in', new_user_session_path, class: 'navbar-brand'
  = link_to 'Sign up', new_user_registration_path, class: 'navbar-brand'
- else
  = link_to 'Profile', (current_user.profilable.kind_of?(PatientProfile) ? patient_profile_path : therapist_profile_path), class: 'navbar-brand'
  = link_to 'Sign out', destroy_user_session_path, class: 'navbar-brand', method: :delete